        {
          "name": "Get-RandomNumber",
          "description": "Generates a random number between 1 and 100.",
          "tab": "",
          "code": "function Get-RandomNumber {$rand =  (Get-Random -Minimum 1 -Maximum 100); $wshell = New-Object -ComObject Wscript.Shell; $wshell.Popup($rand, 0, $rand, 0x1)}"
        }
